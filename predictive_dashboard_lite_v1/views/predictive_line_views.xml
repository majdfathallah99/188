
<odoo>
    <record id="view_predictive_line_tree" model="ir.ui.view">
        <field name="name">predictive.dashboard.line.tree</field>
        <field name="model">predictive.dashboard.line</field>
        <field name="arch" type="xml">
            <tree>
                <field name="wizard_id"/>
                <field name="display_key" string="Key"/>
                <field name="forecast_qty"/>
                <field name="stock_onhand"/>
                <field name="shortage_date"/>
            </tree>
        </field>
    </record>
</odoo>
